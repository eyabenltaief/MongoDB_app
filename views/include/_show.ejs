<% for(var i=0; i < employes.length; i++){ %>
    <tr>
        <td>
            <%= i+1 %>
        </td>
        <td>
            <%= employes[i]._id %>
        </td>
        <td>
            <%= employes[i].nom %>
        </td>
        <td>
            <%= employes[i].prenom %>
        </td>
        <td>
            <%= employes[i].anciennete %>
        </td>
        <td>
            <%= employes[i].adresse %>
        </td>

        <td>
            <%= employes[i].tel %>
        </td>
        <td>
            <%= employes[i].prime %>
        </td>
        <td>
            <a href="/update_user?id=<%= employes[i]._id %>" class="btn border-shadow update">
                <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
            </a>
            <a class="btn border-shadow delete" data-id="<%= employes[i]._id %>">
                <span class="text-gradient"><i class="fas fa-times"></i></span>
            </a>
        </td>
    </tr>
    <% } %>